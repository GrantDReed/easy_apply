<h1>Create job post</h1>
<%= form_for(@job) do |f| %>
  <%= render 'shared/error_messages', instance: @job %>
  <div>
    <%= f.label :title %>
    <%= f.text_field :title %>
  </div>
  <div>
    <%= f.label :description %>
    <%= f.text_area :description %>
  </div>
  <div>
    <%= f.submit "Create post" %>
  </div>
<% end %>